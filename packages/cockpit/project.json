{
  "name": "cockpit",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/cockpit/src",
  "projectType": "library",
  "targets": {
    "build": {
      "dependsOn": ["clean", "^build", "externalize"],
      "executor": "nx:run-script",
      "options": {
        "script": "build"
      }
    },
    "dev": {
      "dependsOn": ["clean", "^build", "externalize"],
      "executor": "nx:run-script",
      "options": {
        "script": "dev"
      }
    },
    "externalize": {
      "executor": "@sydle/ui-nx-plugin:externalize-collections"
    },
    "externalize-undo": {
      "executor": "@sydle/ui-nx-plugin:externalize-collections",
      "options": {
        "undo": true
      }
    }
  }
}
